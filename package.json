{
  "name": "core-design-system",
  "version": "0.1.0",
  "description": "Core design system React components.",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./DXForm": {
      "types": "./dist/types/DXForm/index.d.ts",
      "import": "./dist/esm/DXForm/index.js",
      "require": "./dist/cjs/DXForm/index.cjs"
    },
    "./DXSelect": {
      "types": "./dist/types/DXSelect/index.d.ts",
      "import": "./dist/esm/DXSelect/index.js",
      "require": "./dist/cjs/DXSelect/index.cjs"
    }
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "clean": "node ./scripts/clean.js",
    "build:esm": "webpack --config webpack.config.esm.js",
    "build:cjs": "webpack --config webpack.config.cjs.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "prepublishOnly": "npm run build"
  }
}
